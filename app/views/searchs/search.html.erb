

<div class="col-xs-9">
<% if @user_or_book =="1" %>
	<h1>User search for test</h1>
	<table class="table">
		<tr>
				<th></th>
				<th>Name</th>
				<th>introduction</th>
			</tr>
		<% @users.each do |user| %>
			<tr>
				<td></td>
				<td><%= user.name %></td>
				<td><%= user.introduction %></td>
			</tr>
		<% end %>
	</table>

	<% elsif @user_or_book =="2" %>
	<h2>Book search for test</h2>
	<table class="table">
		<tr>
				<th></th>
				<th>Name</th>
				<th>introduction</th>
			</tr>
		<% @books.each do |book| %>
			<tr>
				<td></td>
				<td><%= book.title %></td>
				<td><%= book.body %></td>
			</tr>
		<% end %>
	</table>
	<% else %>
	<% end %>
</div>
</div>
</div>
